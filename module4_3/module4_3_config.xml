                     <xmpDM:markers>
                        <rdf:Seq>
                           <rdf:li><rdf:Description xmpDM:duration="12000" xmpDM:startTime="0" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Cơ hội để phản hồi là một nhóm các chiến lược gắn kết, cho phép học sinh có cơ \par hội phản hồi các câu hỏi học thuật và tương tác với bạn bè một cách có ý nghĩa và \par có cấu trúc.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="8000" xmpDM:startTime="12000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Học sinh thường mất tập trung, không hoàn thành nhiệm vụ và cư xử sai trái khi bị \par buộc phải ngồi nghe trong thời gian dài.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="9000" xmpDM:startTime="20000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Cơ hội để phản hồi giúp học sinh sử dụng sự tham gia bằng lời nói như một \par phương tiện để đạt được kết quả học tập.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="16000" xmpDM:startTime="29000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Bất kể giáo viên sử dụng chiến lược nào, điều quan trọng là cơ hội để phản hồi nên \par được tích hợp vào từng giai đoạn của bài học để học sinh có cơ hội liên tục xử lý \par và tổng hợp kiến thức.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="7000" xmpDM:startTime="45000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Chọn từng chiến lược để xem mô tả ngắn gọn. Khi bạn đã sẵn sàng, chọn Tiếp tục.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="10500" xmpDM:startTime="71500" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Khi lập kế hoạch một chiến lược cơ hội để phản hồi trong bài học, hãy tự hỏi mình \par những câu hỏi sau. Thứ nhất, tại sao lại chọn cơ hội này? Thứ hai, tại sao nên sử \par dụng nó ngay bây giờ?}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="8000" xmpDM:startTime="83000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Vì mỗi chiến lược có một mục đích khác nhau, chúng nên được sử dụng một cách \par có chủ đích trong các phần khác nhau của bài học.}</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li><rdf:li><rdf:Description xmpDM:duration="8000" xmpDM:startTime="91000" tscDM:valign="bottom" tscDM:halign="center"><xmpDM:name><rdf:Alt><rdf:li xml:lang="en-US">{\rtf1 Cơ hội để phản hồi là một hình thức quản lý chủ động hiệu quả vì chúng làm học \par sinh quá tập trung để cư xử sai trái.\par }</rdf:li></rdf:Alt></xmpDM:name></rdf:Description></rdf:li></rdf:Seq>
